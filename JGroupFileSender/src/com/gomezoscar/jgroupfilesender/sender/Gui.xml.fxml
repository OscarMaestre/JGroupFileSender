<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="261.0" prefWidth="413.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.gomezoscar.jgroupfilesender.sender.GuiFileReceiver">
   <children>
      <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button id="btnSelectFolder" text="%BTN_SELECT_FOLDER_TEXT">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin></Button>
            <Label id="lblDirectory" contentDisplay="TEXT_ONLY" text="Label">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin></Label>
         </children>
      </HBox>
      <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label text="%SELECT_IP">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin></Label>
            <ComboBox fx:id="cmbIpAdresses" maxWidth="1.7976931348623157E308" prefWidth="150.0">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin></ComboBox>
            <Label fx:id="selectedIP" text="%NO_IP_SELECTED">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin></Label>
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button fx:id="btnStartReception" mnemonicParsing="false" text="%START_RECEPTION">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin></Button>
            <Button fx:id="btnStopReception" mnemonicParsing="false" text="%STOP_RECEPTION">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin></Button>
         </children>
      </HBox>
      <VBox prefHeight="200.0" prefWidth="100.0">
         <children>
            <ProgressBar fx:id="receivedFileProgress" maxWidth="1.7976931348623157E308" prefWidth="200.0" progress="0.0" />
            <Label fx:id="lblETA" maxWidth="1.7976931348623157E308" text="%ETA" textAlignment="CENTER" />
         </children>
      </VBox>
      <TextArea fx:id="log" prefHeight="200.0" prefWidth="200.0" />
   </children>
</VBox>
